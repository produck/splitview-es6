    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.20.0",
    "rollup": "^2.70.1",
    "rollup-plugin-terser": "^7.0.2",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  },
  "bugs": {
    "url": "https://github.com/produck/splitview-vue/issues"
